<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DrawBoardView">
        <attr name="dbvMinZoom" format="float"/>
        <attr name="dbvMaxZoom" format="float"/>
        <attr name="dbvFit" format="boolean"/>
        <attr name="dbvDrawEnabled" format="boolean"/>
        <attr name="dbvZoomEnabled" format="boolean"/>
        <attr name="dbvShowTouchPoint" format="boolean"/>
        <attr name="dbvShowSelectedBox" format="boolean"/>
        <attr name="android:src" />
        <attr name="dbvPaintColor" format="color"/>
        <attr name="dbvSelectedBoxColor" format="color"/>
        <attr name="dbvTouchPointColor" format="color"/>
        <attr name="dbvLineStrokeWidth" format="dimension"/>
        <attr name="dbvSelectedStrokeWidth" format="dimension"/>
        <attr name="dbvEraserStrokeWidth" format="dimension"/>
        <attr name="dbvMosaicStrokeWidth" format="dimension"/>
        <attr name="dbvZoomPointStrokeWidth" format="dimension"/>
        <attr name="dbvDrawTextSize" format="dimension"/>
        <attr name="dbvDrawTextBold" format="boolean"/>
        <attr name="dbvDrawTextUnderline" format="boolean"/>
        <attr name="dbvTouchTolerance" format="float"/>
        <attr name="dbvTouchPointRatio" format="float"/>
        <attr name="dbvDrawLineArrow" format="boolean"/>
        <attr name="dbvLineArrowSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RichTextEditor" >
        <attr name="rt_editor_image_height" format="integer" />
        <attr name="rt_editor_image_bottom" format="integer" />
        <attr name="rt_editor_text_init_hint" format="string" />
        <attr name="rt_editor_text_size" format="dimension" />
        <attr name="rt_editor_text_color" format="color" />
        <attr name="rt_editor_text_line_space" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RichTextView" >
        <attr name="rt_view_image_height" format="integer" />
        <attr name="rt_view_image_bottom" format="integer" />
        <attr name="rt_view_text_init_hint" format="string" />
        <attr name="rt_view_text_size" format="dimension" />
        <attr name="rt_view_text_color" format="color" />
        <attr name="rt_view_text_line_space" format="dimension" />
    </declare-styleable>
</resources>